<%= form_with(model: @barber, local: true) do |form| %>
  

  <div class="field">
    <%= form.label :name %>
    <%= form.text_field :name %>
  </div>
  <div class="field">
    <%= form.label :raing %>
    <%= form.text_field :raing %>
  </div>
  <div class="field">
    <%= form.label :phone %>
    <%= form.text_field :phone %>
  </div>


  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>
